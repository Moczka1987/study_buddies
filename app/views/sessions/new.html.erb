
<div class="container">
  <div class="row">

    <h1>Login</h1>

  </div>

  <div class="row login-form">

    <%= form_tag '/login' do%>
    <label>Email</label>
    <%= text_field_tag :email%>
    <label>Password</label>
    <%= password_field_tag :password %>
    <%= submit_tag "Login"%>
    <%end%>

  </div>
</div>
