<%= form_for (@study_group) do |f|%>
<%= f.label :name %>
<%= f.text_field :name%>
<%= f.label :subject_id%>
<%= f.collection_select :subject_id, Subject.all, :id, :name, {:prompt => "Select Subject"}%>
<%= f.label :description%>
<%= f.text_area :description%>
<%= f.label :date%>
<%= f.date_field :date%>
<%= f.label :time%>
<%= f.time_field :time%>
<%= f.label :location_id%>
<%= f.collection_select :location_id, Location.all, :id, :name, {:prompt => "Select Location"}%>
<%= f.label :duration%>
<%= f.number_field :duration%>
<%= f.label :num_participants%>
<%= f.number_field :num_participants%>


<%= f.submit%>
<%end%>
